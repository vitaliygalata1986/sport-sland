{
  "apiVersion": 1,
  "name": "vitos/dynamicblock",
  "title": "Latest Posts",
  "description": "Gutenberg Dynamic Block",
  "category": "text",
  "icon": "admin-customizer",
  "textdomain": "dynamicblock",
  "editorScript": "file:./build/index.js",
  "style": "file:./build/style-index.css",
  "editorStyle": "file:./build/index.css",
  "attributes": {
    "postsPerPage": {
      "type": "number",
      "default": 2
    },
    "showImage": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "string",
      "default": "desc"
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    },
    "category": {
      "type": "number",
      "default": 1
    }
  }
}
